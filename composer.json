{
  "name": "jerry-whoami/filament-form-components",
  "description": "Custom Form Components for Filament",
  "license": "MIT",
  "authors": [
    {
      "name": "JerryWhoami",
      "email": "jerry.whoami@gmail.com",
      "role": "Owner"
    }
  ],
  "require": {
    "php": "^8.2",
    "filament/filament": "^3.0",
    "filament/forms": "^3.2",
    "spatie/laravel-package-tools": "^1.16.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "JerryWhoami\\FilamentFormComponents\\FilamentFormComponentsServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "JerryWhoami\\FilamentFormComponents\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JerryWhoami\\FilamentFormComponents\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
